{
  "name": "No Algorithms",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "Removes algorithmic feeds from websites. No toggles.",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["sites/youtube.js", "parser.js"]
    }
    // {
    //   "matches": ["*://*.instagram.com/*"]
    // }
  ],
  "web_accessible_resources": [{
    "resources": ["sites/block.bettercss"],
    "matches": ["<all_urls>"]
  }]
}
